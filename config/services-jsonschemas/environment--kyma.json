[
    {
        "name": "kymaruntime",
        "plan": "gcp",
        "schemas": {
            "service_instance": {
                "create": {
                    "type": "object",
                    "properties": {
                        "name": {
                            "minLength": 1,
                            "pattern": "^[a-zA-Z0-9-]*$",
                            "title": "Cluster Name",
                            "type": "string",
                            "default": "myKymaCluster"
                        },
                        "region": {
                            "enum": [
                                "asia-northeast1",
                                "europe-west3",
                                "us-central1"
                            ],
                            "type": "string",
                            "title": "Cluster Region",
                            "default": "us-central1"
                        },
                        "autoScalerMin": {
                            "default": 2,
                            "description": "Specifies the minimum number of virtual machines to create",
                            "minimum": 2,
                            "type": "integer",
                            "title": "Minimum number of VMs to create"
                        },
                        "autoScalerMax": {
                            "default": 10,
                            "description": "Specifies the maximum number of virtual machines to create",
                            "maximum": 40,
                            "minimum": 2,
                            "type": "integer",
                            "title": "Maximum number of VMs to create"
                        },
                        "machineType": {
                            "enum": [
                                "n2-standard-8",
                                "n2-standard-16",
                                "n2-standard-32",
                                "n2-standard-48"
                            ],
                            "type": "string",
                            "title": "Type of VM",
                            "default": "n2-standard-8"
                        }
                    },
                    "required": [
                        "name"
                    ]
                }
            }
        }
    },
