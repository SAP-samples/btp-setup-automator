{
  "name": "privatelink",
  "description":  "The SAP Private Link service (BETA) establishes a private connection between selected SAP BTP services and selected services in your own IaaS provider accounts. By reusing the private link functionality of our partner IaaS providers, it lets you access your services through private network connections to avoid data transfer via the public Internet.",
  "displayName": "Private Link Service",
  "icon": "data:image/svg+xml;base64,",
  "links": [
    {
        "linkURL": "https://help.sap.com/viewer/product/PRIVATE_LINK/CLOUD/en-US",
        "title": "Documentation"
    },
    {
        "linkURL": "https://support.sap.com/en/index.html",
        "title": "Support information"
    }
],
  "servicePlans": [{
      "name": "Establish a private connection between your CF application and selected services in your own IaaS provider accounts.",
      "displayName": "Establish a private connection between your CF application and selected services in your own IaaS provider accounts.",
      "category": "SERVICE",
      "description": "Establish a private connection between your CF application and selected services in your own IaaS provider accounts.",
      "schemas": {
    "service_binding": {
        "create": {
            "parameters": null
        }
    },
    "service_instance": {
        "create": {
            "parameters": {
                "$schema": "http://json-schema.org/draft-06/schema#",
                "_controlsOrder": [
                    "resourceId",
                    "subResource",
                    "requestMessage"
                ],
                "_show_form_view": true,
                "additionalProperties": false,
                "properties": {
                    "requestMessage": {
                        "description": "Specifies the request message which is displayed to the approver.",
                        "maxLength": 60,
                        "title": "Request Message",
                        "type": "string"
                    },
                    "resourceId": {
                        "description": "Specifies the Azure Resource ID of the service for which the private endpoint should be created.\n\nExample: /subscriptions/\u003csubscription\u003e/resourceGroups/\u003crg\u003e/providers/Microsoft.Network/privateLinkServices/\u003cmyPrivateLinkService\u003e",
                        "minLength": 1,
                        "pattern": "/subscriptions/.+/resourceGroups/.+/providers/.+?/.+?/[^/#?]+",
                        "title": "Resource ID",
                        "type": "string"
                    },
                    "subResource": {
                        "description": "Specifies the subresource for a specific Resource ID. Note that subresource is not required for Azure Private Link resources.\n\nExample: mysqlServer\n\nAll supported native Azure services including subresources can be found here: https://help.sap.com/viewer/product/PRIVATE_LINK/CLOUD/en-US",
                        "title": "Subresource",
                        "type": "string"
                    }
                },
                "required": [
                    "resourceId"
                ],
                "type": "object"
            }
        },
        "update": {
            "parameters": null
        }
    }
},
